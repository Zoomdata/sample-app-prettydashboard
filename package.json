  "license": "ISC",
  "dependencies": {
    "ag-grid": "^4.0.5",
    "ag-grid-react": "^4.0.0",
    "babel-polyfill": "^6.7.2",
    "compression": "^1.6.1",
    "echarts": "^3.1.3",
    "if-env": "^1.0.0",
    "jquery": "^2.2.3",
    "lodash": "^4.6.1",
    "moment": "^2.12.0",
    "numeral": "^1.5.3",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dimensions": "^1.0.2",
    "react-dom": "^0.14.7",
    "react-highcharts": "^8.0.3",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
